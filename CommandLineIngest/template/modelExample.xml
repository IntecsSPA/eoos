<model>
<!--
"SATELLITE_NO","SENSOR_ID","PRODUCT_TYPE","SENSOR_MODE","PRODUCT_ID","PROVIDER","ROLE","MISSION_PHASE","ORBIT","LAST_ORBIT","ORBIT_DIRECTION","TRACK","FRAME","FRAME_START","FRAME_STOP","SWATH_ID","ACROSS_POINTING_ANGLE","ALONG_POINTING_ANGLE","ELEVATION_ANGLE","AZIMUTH_ANGLE","TANGENT_HEIGHT","TANGENT_LAT","TANGENT_LONG","SOLAR_LAT","SOLAR_LONG","STAR_NAME","POLARISATION","STATION","PROJ_CENTER_X","PROJ_CENTER_Y","PROJ_CENTER_Z","ROLL","PITCH","YAW","STATUS","INTERNAL_STATUS","BEGIN_DATE","BEGIN_MSEC","END_DATE","END_MSEC","SCAN_LINE_POINTS","VERTICAL_COVERAGE_MIN_HEIGHT","VERTICAL_COVERAGE_MAX_HEIGHT","REGION_ID","LINES","PROCESSING_LEVEL","BAND_NAME","BAND_GAIN","BAND_QUALITY","BAND_MODE","PROCESSING_TYPE","PROCESSING_DATE_TIME","PROCESSING_CENTRE","PROCESSING_CODE","SCENE_SELECTION_TYPE","SEL_SCENE_CENTRE_LAT","SEL_SCENE_CENTRE_LONG","SUB_SCENE_ID","ALONG_SHIFT","ALONG_SHIFT_UNIT_ID","ALONG_SIZE","ALONG_SIZE_UNIT_ID","SCENE_SELECTION_BEGIN_DATE","SCENE_SELECTION_BEGIN_MSEC","SCENE_SELECTION_END_DATE","SCENE_SELECTION_END_MSEC","PROCESSING_ALGORITHM_VER","URL","IMAGE_QUALITY_CODE","WARNING_MESSAGE","VALIDITY_FIELD","NOTIFICATION_FILE_NAME","LAST_UPDATE","DATASET_URL","PAC_3CC","STATION_3CC","METADATA_FILENAME","CREATION_DATE","INGESTION_DATE","LA_SIZE","LA_VIDEO_FORMAT","LA_HOR_SHIFT","LA_ACQ_VOTE","LA_SATUR_VOTE","LA_COMPRESSION_MODE","LA_EARTH_ROTATION_CORRECTION","LA_POINTING_CHANGE_FLAG","LA_VIEW_EXACT_ANGLE","LA_GAIN_STATUS","LA_EXPOSURE_COEFF_STATUS","LA_REV_CORRECTION","LA_PERPENDICULAR_DISTANCE","LA_PARALLEL_DISTANCE","LA_GRID_SPACING","LA_COMMAND_LINE_PARAMETERS","LA_SOURCE_FILETYPE","LA_RESOLUTION","LA_OCC_DURATION","LA_OCC_NUMBER","POLYGON_STORED","SCENE_CENTRE_STORED"
-->
	<generalConstant name="urlRoot">http://root-path-to-landsat-products/</generalConstant>
<!-- Dataset identification-->
	<attribute id="platformShortName">
	<indexFieldName>PLATFORM_ID</indexFieldName>
		<!--defaultValue>LS01</defaultValue-->
	</attribute>
	<attribute id="instrumentShortName">
		<indexFieldName>SENSOR_ID</indexFieldName>
	</attribute>
	<attribute id="operationalMode">
		<indexFieldName>SENSOR_MODE</indexFieldName>
	</attribute>
	<attribute id="productType">
		<indexFieldName>PRODUCT_TYPE</indexFieldName>
	</attribute>
<!-- Product identification-->
	<attribute id="productId">
		<indexFieldName>PRODUCT_ID</indexFieldName>
	</attribute>
<!-- Product date-time-->
	<attribute id="beginAcquisition">
		<indexFieldName>BEGIN_DATE</indexFieldName>
	</attribute>
	<attribute id="endAcquisition">
		<indexFieldName>END_DATE</indexFieldName>
	</attribute>
	<attribute id="availabilityTime">
		<formula>$date.get('yyyy-M-d')T$date.get('H:m:s')Z</formula>
	</attribute>
<!-- Product footprint-->
	<attribute id="footprint">
		<indexFieldName>POLYGON_STORED</indexFieldName>
	</attribute>
<!-- Product footprint-->
	<attribute id="productURI">
		<formula>$urlRoot${metadata.PRODUCT_ID}.zip</formula>
	</attribute>
	<attribute id="productVersion">
		<defaultValue>VERSION</defaultValue>
	</attribute>
	<attribute id="productSize">
		<indexFieldName>LA_SIZE</indexFieldName>
	</attribute>
<!-- Other product metadata-->
	<attribute id="sensorType">
		<defaultValue>RADAR</defaultValue>
	</attribute>
	<attribute id="processingMode">
		<indexFieldName>PROCESSING_TYPE</indexFieldName>
	</attribute>
	<attribute id="acquisitionType">
		<defaultValue>ACQUISITION_TYPE</defaultValue>
	</attribute>
	<attribute id="acquisitionSubType">
		<defaultValue>ACQUISITION_SUB_TYPE</defaultValue>
	</attribute>
	<!--attribute id="productType"> IS THIS REPEATED??
		<defaultValue>DEFAULT</defaultValue>
	</attribute-->
	<attribute id="Status">
		<indexFieldName>STATUS</indexFieldName>
	</attribute>
	<attribute id="imageQualityDegradation">
		<indexFieldName>IMAGE_QUALITY_CODE</indexFieldName>
	</attribute>
	<attribute id="imageQualityStatus">
		<defaultValue>DEFAULT</defaultValue>
	</attribute>
	<attribute id="imageQualityDegradationTag">
		<defaultValue>DEFAULT</defaultValue>
	</attribute>
	<attribute id="imageQualityReportURL">
		<defaultValue>http://DEFAULT</defaultValue>
	</attribute>
	<attribute id="productGroupId">
		<defaultValue>DEFAULT_GROUP</defaultValue>
	</attribute>
	<attribute id="platformSerialIdentifier">
		<defaultValue>PLATFORM_SERIAL_ID</defaultValue>
	</attribute>
	<attribute id="swathIdentifier">
		<indexFieldName>SWATH_ID</indexFieldName>
	</attribute>
	<attribute id="orbitNumber">
		<indexFieldName>ORBIT</indexFieldName>
	</attribute>
	<attribute id="orbitDirection">
		<indexFieldName>ORBIT_DIRECTION</indexFieldName>
	</attribute>
	<attribute id="wrsLongitudeGrid">
		<defaultValue>DEFAULTLON_GRID</defaultValue>
	</attribute>
	<attribute id="wrsLatitudeGrid">
		<defaultValue>DEFAULT_LAT_GRID</defaultValue>
	</attribute>
	<attribute id="startTimeFromAscendingNode">
		<defaultValue>200000000000</defaultValue>
	</attribute>
	<attribute id="completionTimeFromAscendingNode">
		<defaultValue>2000000000002222</defaultValue>
	</attribute>
	<attribute id="illuminationAzimuthAngle">
		<indexFieldName>AZIMUTH_ANGLE</indexFieldName>
	</attribute>
	<!--attribute id="illuminationZenithAngle">
		<defaultValue>DEFAULT</defaultValue>
	</attribute-->
	<attribute id="illuminationElevationAngle">
		<indexFieldName>ELEVATION_ANGLE</indexFieldName>
	</attribute>	
<!-- metadata for OPTICAL or ATMOSPHERIC products -->	
	<attribute id="cloudCoverPercentage">
		<defaultValue>DEFAULT</defaultValue>
	</attribute>
	<attribute id="snowCoverPercentage">
		<defaultValue>DEFAULT</defaultValue>
	</attribute>

<!-- metadata for ATMOSPHERIC products -->	
	<attribute id="multiViewAngles">
		<defaultValue>DEFAULT</defaultValue>
	</attribute>
	<attribute id="centreViewAngles">
		<defaultValue>DEFAULT</defaultValue>
	</attribute>
<!-- metadata for RADAR products -->	
	<attribute id="polarisationMode">
		<indexFieldName>POLARIZATION</indexFieldName>
	</attribute>
	<!--attribute id="polarisationChannels">
		<defaultValue>DEFAULT</defaultValue>
	</attribute>
		<attribute id="antennaLookDirection">
		<defaultValue>DEFAULT</defaultValue>
	</attribute>
	<attribute id="minimumIncidenceAngle">
		<defaultValue>DEFAULT</defaultValue>
	</attribute>
		<attribute id="maximumIncidenceAngle">
		<defaultValue>DEFAULT</defaultValue>
	</attribute>
	<attribute id="incidenceAngleVariation">
		<defaultValue>DEFAULT</defaultValue>
	</attribute>
		<attribute id="dopplerFrequency">
		<defaultValue>DEFAULT</defaultValue>
	</attribute-->
<!-- metadata for ALTIMETRIC and LIMB products -->	
	<attribute id="nominalTrack">
		<defaultValue>DEFAULT</defaultValue>
	</attribute>
<!-- metadata for LIMB products-->	
	<attribute id="occultationPoints">
		<defaultValue>DEFAULT</defaultValue>
	</attribute>
<!-- Additional product medata-->	
	<attribute id="add-pippo">
		<defaultValue>PIPPO</defaultValue>
	</attribute>
	<attribute id="add-pluto">
		<defaultValue>PLUTO</defaultValue>
	</attribute>

<!-- Browse metadata-->	
	<attribute id="browseImageLocation">
		<defaultValue>DEFAULT</defaultValue>
	</attribute>
	<attribute id="browseMetadataLocation">
		<defaultValue>DEFAULT</defaultValue>
	</attribute>
<!-- NOT MAPPED IN THE DOCUMENT-->	
	<attribute id="dateType">
		<indexFieldName>INGESTION_TIME</indexFieldName>
	</attribute>



</model>
